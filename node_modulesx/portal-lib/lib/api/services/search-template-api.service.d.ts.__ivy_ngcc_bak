import { HttpClient } from '@angular/common/http';
import { SearchTemplate } from '../model/search-template.model';
import { Observable } from 'rxjs';
export declare class SearchTemplateAPIService {
    private httpClient;
    private readonly baseUrl;
    private readonly httpOptions;
    constructor(httpClient: HttpClient);
    getSearchTemplatesLegacy(user: string, application: string, page: string, includeGlobal?: boolean): Observable<SearchTemplate[]>;
    getTemplateById(id: string): Observable<SearchTemplate>;
    createNewTemplate(template: SearchTemplate): Observable<SearchTemplate>;
    updateTemplate(id: string, template: SearchTemplate): Observable<SearchTemplate>;
    deleteTemplate(id: string): Observable<boolean>;
}
