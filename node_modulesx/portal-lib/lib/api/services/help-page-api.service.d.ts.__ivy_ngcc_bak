import { HttpClient, HttpHeaders, HttpResponse } from '@angular/common/http';
import { HelpData } from '../model/help-data.model';
import { Observable } from 'rxjs';
export declare class HelpPageAPIService {
    private httpClient;
    httpOptions: {
        headers: HttpHeaders;
    };
    constructor(httpClient: HttpClient);
    getHelpDataItem(appId: string, helpItemId: string, type?: string): Observable<HelpData>;
    getHelpDataItemLegacy(appId: string, context: string, type?: string): Observable<HelpData>;
    saveHelpPage(appId: string, helpItem: HelpData): Observable<HttpResponse<any>>;
    saveHelpPageLegacy(helpItem: HelpData): Observable<HttpResponse<any>>;
}
