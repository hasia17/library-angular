import { OnInit } from '@angular/core';
import { Observable } from 'rxjs';
export declare abstract class PortalSearchPage<T, C> implements OnInit {
    searchInProgress: boolean;
    results: T[];
    criteria: C;
    abstract doSearch(): T[] | Observable<T[]>;
    abstract getDefaultCriteria(): C;
    search(): void;
    reset(): void;
    ngOnInit(): void;
}
