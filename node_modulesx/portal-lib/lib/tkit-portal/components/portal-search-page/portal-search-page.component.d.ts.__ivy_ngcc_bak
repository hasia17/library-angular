import { EventEmitter, Injector } from '@angular/core';
import { SelectItem } from 'primeng/api';
import { MessageService } from 'primeng/api';
import { SearchTemplate } from '../../../api/model/search-template.model';
import { PortalPageComponent } from '../portal-page/portal-page.component';
import { SearchTemplateAPIService } from '../../../api/services/search-template-api.service';
import { IAuthService } from '../../../api/iauth.service';
import { PortalViewportComponent } from '../portal-viewport/portal-viewport.component';
import { PortalSearchPage } from '../../pages/PortalSearchPage';
export declare class PortalSearchPageComponent extends PortalPageComponent {
    private messageService;
    private searchTemplateApi;
    private auth;
    private viewPort;
    searchPage: PortalSearchPage<any, any>;
    searchCriteriaTemplatesEnabled: boolean;
    enableSearchToolbar: boolean;
    onSearch: EventEmitter<any>;
    activeSearchTemplate: SearchTemplate;
    activeSearchTemplateId: string;
    private pageName;
    searchTemplateOptions: SelectItem[];
    newTemplateName: string;
    newTemplateDefaultFlag: boolean;
    constructor(injectorChild: Injector, messageService: MessageService, searchTemplateApi: SearchTemplateAPIService, auth: IAuthService, viewPort: PortalViewportComponent, searchPage: PortalSearchPage<any, any>);
    ngOnInit(): void;
    onSearchTemplateSelection(event: any): void;
    private fetchSearchTemplates;
    validatedInputs(): void;
    triggerSearch(): void;
    triggerCriteriaTemplateUpdate(): void;
    triggerCriteriaTemplateSave(event: Event, op: any): void;
    triggerCriteriaTemplateDelete(): void;
}
