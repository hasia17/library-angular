import { ViewContainerRef, TemplateRef, OnInit } from '@angular/core';
import { IAuthService } from '../../api/iauth.service';
export declare class TkIfPermDirective implements OnInit {
    private templateRef;
    private viewContainer;
    private authService;
    permission: string;
    loaderState: boolean;
    constructor(templateRef: TemplateRef<any>, viewContainer: ViewContainerRef, authService: IAuthService);
    set tkIfPerm(permission: string);
    ngOnInit(): void;
}
