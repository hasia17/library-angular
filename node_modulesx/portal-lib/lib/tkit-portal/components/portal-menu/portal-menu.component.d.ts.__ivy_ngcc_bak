import { EventEmitter, OnDestroy, OnInit } from '@angular/core';
import { MenuItem } from 'primeng/api';
import { MenuService } from '../../../api/services/menu.service';
import { Subscription } from "rxjs";
import { Router } from "@angular/router";
export declare class AppMenuComponent {
    menuActive: boolean;
    isStatic: boolean;
    isHorizontal: boolean;
    isMobile: boolean;
    isDesktop: boolean;
    isSlim: boolean;
    menuHoverActive: boolean;
    mobileMenuActive: boolean;
    set portalMenu(items: MenuItem[]);
    menuActiveChange: EventEmitter<boolean>;
    mobileMenuActiveChange: EventEmitter<boolean>;
    menuWithRoot: MenuItem;
    constructor();
}
export declare class AppSubMenuComponent implements OnInit, OnDestroy {
    private menuService;
    private router;
    item: MenuItem;
    root: boolean;
    visible: boolean;
    index: number;
    parentKey: string;
    menuActive: boolean;
    isStatic: boolean;
    isHorizontal: boolean;
    isMobile: boolean;
    isDesktop: boolean;
    isSlim: boolean;
    menuHoverActive: boolean;
    mobileMenuActive: boolean;
    menuActiveChange: EventEmitter<boolean>;
    mobileMenuActiveChange: EventEmitter<boolean>;
    active: boolean;
    menuSourceSubscription: Subscription;
    menuResetSubscription: Subscription;
    key: string;
    constructor(menuService: MenuService, router: Router);
    ngOnInit(): void;
    ngOnDestroy(): void;
    updateActiveStateFromRoute(): void;
    itemClick(event: Event): boolean;
    onMouseEnter(): void;
}
