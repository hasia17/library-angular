import { EventEmitter, OnInit } from '@angular/core';
import { IAuthService } from '../../api/iauth.service';
import { UserProfile } from '../../api/model/user-profile.model';
import { AvatarInfo } from '../../api/model/avatar-info.model';
import { Router } from '@angular/router';
import { ConfigurationService } from '../../api/configuration.service';
export declare class AppInlineProfileComponent implements OnInit {
    private authService;
    private router;
    private configService;
    defaultAvatarUrl: string;
    userProfile: UserProfile;
    userAvatar: AvatarInfo;
    inlineMenuActive: boolean;
    inlineMenuClick: EventEmitter<null>;
    get displayName(): string;
    get baseUrl(): string;
    get tabIndex(): string;
    constructor(authService: IAuthService, router: Router, configService: ConfigurationService);
    ngOnInit(): void;
    onClick(event: any): void;
    logout(event: Event): void;
}
