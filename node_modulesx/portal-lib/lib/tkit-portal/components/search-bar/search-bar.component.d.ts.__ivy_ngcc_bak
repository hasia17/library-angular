import { AfterViewInit, EventEmitter, OnInit } from '@angular/core';
import { PortalItemService } from '../../../api/services/portal-item.service';
import { SearchItemDTO } from '../../../api/model/search-item.model';
import { FormControl } from '@angular/forms';
import { SearchItemType } from '../../../api/model/search-item-type';
import { ConfigurationService } from '../../../api/configuration.service';
import { TranslateService } from '@ngx-translate/core';
import { MessageService } from 'primeng/api';
import { AutoComplete } from 'primeng/autocomplete';
export declare class SearchBarComponent implements OnInit, AfterViewInit {
    private searchService;
    private config;
    private readonly translateService;
    private readonly messageService;
    results: SearchItemDTO[];
    query: FormControl;
    SearchItemType: typeof SearchItemType;
    searchUrl: string;
    maxResults: 10;
    searchInputViewChild: AutoComplete;
    searchClick: EventEmitter<any>;
    get openInSearchItem(): SearchItemDTO;
    constructor(searchService: PortalItemService, config: ConfigurationService, translateService: TranslateService, messageService: MessageService);
    ngOnInit(): void;
    ngAfterViewInit(): void;
    search(event: {
        query: string;
    }): void;
    select(event: SearchItemDTO): void;
    goToSearch(clickEnter: boolean): void;
    onSearchClick(event: any, val: boolean): void;
}
