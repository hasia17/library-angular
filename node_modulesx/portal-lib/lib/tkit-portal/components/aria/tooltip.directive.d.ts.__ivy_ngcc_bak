import { ElementRef, NgZone, AfterViewInit } from '@angular/core';
import { Tooltip } from 'primeng/tooltip';
export declare class TooltipAriaDirective extends Tooltip implements AfterViewInit {
    el: ElementRef;
    zone: NgZone;
    constructor(el: ElementRef, zone: NgZone);
    ngAfterViewInit(): void;
    private initHoverForTooltipEvent;
    private initFocusForTooltipEvent;
    set pTooltipText(text: string);
}
